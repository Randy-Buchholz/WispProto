self.Styling = class Styling {
   Space;
   Skel;
   Skin;

   constructor({ Space = "", Skel = "", Skin = "" }) {
      this.Space = Space;
      this.Skel = Skel;
      this.Skin = Skin;
   }

   toString() {
      return clean([
         clean(this.Space),
         clean(this.Skel),
         clean(this.Skin)
      ]);
   }

   get stylette() {
      return this.toString();
   }
};
